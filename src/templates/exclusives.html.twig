{% extends "partials/base.html.twig" %}
{% block title %}Exclusives{% endblock %}

{% block content %}

<div class="wrappercontainer">
		<div class="wrapper" id="We run tings">
			<h3>We run tings</h3>
			<audio class="myTrack">
				<source src="./audio/We run tings.wav" type="audio/mp3" />
				<source src="./audio/We run tings.ogg" type="audio/ogg" />
			</audio>

			<div class="buttons">
				<button type="button" class="playButton"></button>
			</div>

			<div class="timebox">
				<span class="current">00:00</span>
				
				<progress class="progressBar" value="0" max="100"></progress> 
				
				<span class="duration">00:00</span>
			</div>
		</div>

		<div class="wrapper" id="Danger">
			<h3>Will Robinson</h3>
			<audio class="myTrack">
				<source src="./audio/Will Robinson.mp3" type="audio/mp3" />
				<source src="./audio/Will Robinson.ogg" type="audio/ogg" />
			</audio>

			<div class="buttons">
				<button type="button" class="playButton"></button>
			</div>

			<div class="timebox">
				<span class="current">00:00</span>
				
				<progress class="progressBar" value="0" max="100"></progress> 
				
				<span class="duration">00:00</span>
			</div>
		</div>

		<div class="wrapper" id="Buh!">
			<h3>Coming on strong</h3>
			<audio class="myTrack">
				<source src="./audio/Coming on strong.mp3" type="audio/mp3" />
				<source src="./audio/Coming on strong.ogg" type="audio/ogg" />
			</audio>

			<div class="buttons">
				<button type="button" class="playButton"></button>
			</div>

			<div class="timebox">
				<span class="current">00:00</span>
				
				<progress class="progressBar" value="0" max="100"></progress> 
				
				<span class="duration">00:00</span>
			</div>
		</div>
</div>
<script type="text/javascript" src="/js/playpause.js" defer></script>

{% endblock %}