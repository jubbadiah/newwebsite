{% extends "partials/base.html.twig" %}
{% block title %}SignUp{% endblock %}

{% block content %}
    
    {% if completed %}
<div class="account">
        <h1>Thanks for signing up</h1>
            <a href="/login">Click here to login</a>
</div>
    {% else %}

    {{ form_start(form) }}
    {{ form_widget(form) }}



        <input type="submit" value="Sign Up">

    {# <div class="my-custom-class-for-errors">
    {{ form_errors(form) }}
    </div> #}

    {{ form_end(form) }}

    {% endif %}



{% endblock %}